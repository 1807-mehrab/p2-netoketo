<div class="container">
  <div class="row">
    <div class="col-md-6">
      <div class="container" style="background-color: #f4f4f4">
        <p>
          Username
        </p>
        <p>
          Email
        </p>
      </div>
    </div>


    <div class="col-md-6">
      <p>
        {{#each model as |user|}}
        <p>{{user.username}}</p>
        <p>{{user.password}}</p>
        <p>{{user.email}}</p>
        <p>{{user.userType}}</p>
        {{/each}}
      </p>
    </div>
  </div>
  {{#link-to "editprofile"}}
    Edit Profile
  {{/link-to}}
</div>
