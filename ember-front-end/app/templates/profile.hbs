<div class="container">
  <div class="row">
    <div class="col-md-4">
      <h2>
        Profile
      </h2>
      <a href="https://placeholder.com"><img src="https://via.placeholder.com/350x150"></a>
      {{#each model as |login|}}
      <h4>Username : {{login.username}}</h4>
      {{/each}}
    </div>
    <div class="col-md-8">
      <br />
      {{#each model as |login|}}
      <h4>Email : {{login.email}}</h4>
      {{/each}}
      <br />
      <h4>Recipes made:</h4>
      <br />
      <h4>Liked Recipes:</h4>

    </div>
  </div>
  {{#link-to "editprofile"}}
    Edit Profile
  {{/link-to}}
</div>
